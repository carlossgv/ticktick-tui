{
	"name": "ticktick-tui",
	"repository": {
		"type": "git",
		"url": "github:carlossgv/ticktick-tui.git"
	},
	"version": "0.1.0-alpha.11",
	"license": "MIT",
	"bin": {
		"ticktick-tui": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"release": "release-it"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"axios": "^1.9.0",
		"chrono-node": "2.7.5",
		"fullscreen-ink": "^0.0.2",
		"ink": "^5.2.1",
		"luxon": "^3.6.1",
		"meow": "^11.0.0",
		"prompts": "^2.4.2",
		"react": "^18.2.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.27.0",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/luxon": "^3.6.2",
		"@types/prompts": "^2.4.9",
		"@types/react": "^18.0.32",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint": "^9.27.0",
		"eslint-config-xo-react": "^0.28.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.1.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^3.5.3",
		"release-it": "^19.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"typescript-eslint": "^8.32.1",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
